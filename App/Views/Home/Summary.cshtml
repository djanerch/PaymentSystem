@model IEnumerable<App.Data.Models.Summary>

@{
    ViewData["Title"] = "Summary";
}

<h1>Справка с неплатилите от минали години</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Цяло име:
            </th>
            <th>
                Графа:
            </th>
            <th>
                Година:
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>
        </tr>
}
    </tbody>
</table>
